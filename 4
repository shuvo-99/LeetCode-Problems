class Solution:
    def findMedianSortedArrays(self, nums1: List[int], nums2: List[int]) -> float:
        m=[]
        for i in nums1:
            m.append(i)
        for i in nums2:
            m.append(i)
        m.sort()    
        if len(m)%2!=0:
            median = m[int((len(m)+1)/2)-1]
            return median
        else:
            m1= m[int((len(m)/2))-1]
    
    
            m2 = m[int((len(m)/2)+1)-1]
    
            median = (m1+m2)/2
            return median
